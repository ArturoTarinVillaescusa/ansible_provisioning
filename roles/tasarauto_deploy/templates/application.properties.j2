#Propiedades obligatorias micro servicio
rentalcar.tasks.endPoints = ,/Tasks,/Tasks/ComunicacionesPendientes,/Tasks/DatosVehiculosRecompra,/Tasks/DatosVehiculosVentas
rentalcar.tasks.nameMethods = UrlBase,Tasks,comunicacionespendientes,datosvehiculosrecompra,datosvehiculosventas
rentalcar.uris.endPoints = /Tasks
rentalcar.uris.nameMethods = Tasks
rentalcar.errors.codes =
rentalcar.errors.descriptions =
rentalcar.daemon = 1
log4j.category.org.apache.http.wire=DEBUG
log4j.logger.org.hibernate=OFF
spring.jpa.show-sql=false

#logging
logging.file=Logs/Log.log
logging.level = INFO
logging.file.max-size=10MB
logging.file.max-history =10
logging.level.org.hibernate=ERROR
springfox.documentatation.swagger.v2.path:/swagger.json

# Configuraciï¿½n HTTPS
server.port: {{ tasarauto_port }}

# Notificacion fop
rentalcar.provider.endPoint.fop={{ tasarauto_ws_endpoint_fop }}

# Notificacion importe franquicia
rentalcar.provider.endPoint.franchise={{ tasarauto_ws_endpoint_franchise }}

# Login
rentalcar.provider.endPoint.login={{ tasarauto_ws_endpoint_login }}

# Bloqueo o desbloqueo
rentalcar.provider.endPoint.lock={{ tasarauto_ws_endpoint_lock }}

# Envio de fichero de vehiculos
rentalcar.provider.endPoint.vehicleData={{ tasarauto_ws_endpoint_vehicleData }}

# Usuario que consume el WS de Tasarauto
rentalcar.provider.auth.user={{ tasarauto_ws_user }}