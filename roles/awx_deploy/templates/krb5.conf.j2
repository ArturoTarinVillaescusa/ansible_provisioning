[logging]
 default = FILE:/var/log/krb5libs.log
 kdc = FILE:/var/log/krb5kdc.log
 admin_server = FILE:/var/log/kadmind.log

[libdefaults]
 dns_lookup_realm = false
 ticket_lifetime = 24h
 renew_lifetime = 7d
 forwardable = true
 default_realm = LABCEN.LOCAL
 default_ccache_name = KEYRING:persistent:%{uid}
 allow_weak_crypto = true

[realms]
 LABCEN.LOCAL = {
  kdc = adddns01.labcen.local:88
  default_domain = LABCEN.LOCAL
  admin_server = adddns01.labcen.local
 }

[domain_realm]
 .labcen.local = LABCEN.LOCAL
 labcen.local = LABCEN.LOCAL
